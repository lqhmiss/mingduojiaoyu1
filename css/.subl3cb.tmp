
               /*全局样式*/
.header{
	margin-bottom: 20px;
}
.logo{
	overflow: hidden;
}

.logo h1{
     float: left;
}
.logo a{
	display: block;
	width: 217px;
	height: 63px;
	text-indent: -9999px;
	background: url(../images/logo.gif);
	
}
.logo dt{
	font-weight: bold;
	opacity: 0.7;
}

.logo dl{
	 float: right;
	 margin-top: 15px;

}
.logo dd{
	color: #666;
	margin-top: 5px;
	font-size: 12px;
}

.header ul{
	width: 920px;
	height: 36px;
	margin-top: 10px;
	background: url(../images/navbg.gif) no-repeat;
}
.header li{
	float: left;
	width: 134px;
	line-height: 36px;
	text-align: center;
	background-repeat: no-repeat;
	background-position: center;
    
}
.header li a{
	color: #fff;
	font-weight: bold;
	text-decoration: none;
}

.header .current{
	width: 88px;
	height: 32px;
	margin-left: 48px;
	background-image: url(../images/home.gif);
	background-position: 0 5px;
}
.header .current a{
	color: #000;
}
.header .lab{
	float: right;
	margin-right: 20px;
	background-image: url(../images/arrow_r.gif);
	background-position: 10px 11px;
}
.main{
	overflow: hidden;
	margin-bottom: 20px;
}
.main-left{
	width: 600px;
	background: #666;
	float: left;
}
.list h3{
	border-bottom: 1px solid #0ff;
	font-size: 14px;
	padding: 5px 0px;
	margin-top: 5px;
	color: #266392;

}
.list h3 a{
	float: right;
	font-size: 12px;
}
.list dl{
	padding: 10px 50px;
	float: left;
	background-image: url(../images/icon_1.gif) ;
	background-repeat: no-repeat;
	background-position: 1px 10px;

}
.list dt{
	color: 666;
	font-size: 12px;
}
.list dt a{
	font-size: 14px;
	color: 266392;
	font-weight: bold;
    
}
.list span{
	width: 100px;
	padding-top: 10px;
	float: right;
	background-image: url(../images/downicon.gif) ;
	background-repeat:  no-repeat;
	background-position: 0px 15px;
}









.main-right{
	float: right;
	width: 280px;
	background: #0ff;

}






/*<!-- <footer尾部> -->*/
.footer{
	width: 870px;
	padding: 20px;
	font-size: 12px;
	text-align: center;
	border-top: 1px solid #e6e6e6;
	background-color: #f6f6f6;

}
.footer p{
	margin-bottom: 10px;
	color: 9c9c9c;
}
